{
    "env": {"node": true},
    "globals": {},
    "rules": {
      "no-extra-parens": 2,
      "no-unexpected-multiline": 2,
      "valid-jsdoc": 2,
      "default-case": 2,
      "dot-notation": 2,
      "eqeqeq": 2,
      "no-alert": 2,
      "no-caller": 2,
      "no-else-return": 2,
      "no-empty-label": 2,
      "no-eval": 2,
      "no-extend-native": 2,
      "no-floating-decimal": 2,
      "no-implied-eval": 2,
      "no-iterator": 2,
      "no-lone-blocks": 2,
      "no-loop-func": 2,
      "no-multi-spaces": 2,
      "no-multi-str": 2,
      "no-native-reassign": 2,
      "no-new-func": 2,
      "no-new-wrappers": 2,
      "no-new": 2,
      "no-octal-escape": 2,
      "no-proto": 2,
      "no-return-assign": 2,
      "no-self-compare": 2,
      "no-throw-literal": 2,
      "no-unused-expressions": 2,
      "no-warning-comments": 2,
      "no-with": 2,
      "radix": 2,
      "strict": [2, "global"],
      "no-shadow-restricted-names": 2,
      "no-shadow": 2,
      "no-undef-init": 2,
      "no-use-before-define": 2,
      "callback-return": 2,
      "handle-callback-err": 2,
      "no-mixed-requires": 2,
      "no-new-require": 2,
      "no-path-concat": 2,
      "no-process-exit": 2,
      "no-sync": 2,
      "camelcase": 2,
      "consistent-this": [2, "self"],
      "indent": 2,
      "linebreak-style": 2,
      "new-cap": 2,
      "new-parens": 2,
      "no-array-constructor": 2,
      "no-fallthrough": 1,
      "no-multiple-empty-lines": [2, {"max": 2}],
      "no-nested-ternary": 2,
      "no-new-object": 2,
      "no-trailing-spaces": 2,
      "quotes": [2, "single", "avoid-escape"],
      "semi": [2, "never"],
      "space-return-throw-case": 2,
      "spaced-comment": [2, "always", {"exceptions": ["="]}]
    }
}
