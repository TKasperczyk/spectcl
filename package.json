{
  "name": "spectcl",
  "description": "Spawns and interacts with child processes using spawn / expect commands",
  "version": "0.0.8",
  "author": "Greg Cochard <greg@gregcochard.com>",
  "maintainers": [
    "Greg Cochard <greg@gregcochard.com>",
    "Ryan Milbourne <ryanbmilbourne@gmail.com>"
  ],
  "repository": {
    "type": "git",
    "url": "http://github.com/spectcl/spectcl.git"
  },
  "keywords": [
    "child process",
    "expect",
    "interactive",
    "spawn",
    "terminal"
  ],
  "devDependencies": {
    "colors": "0.x.x",
    "coveralls": "^2.11.2",
    "dateformat": "^1.0.11",
    "eslint": "^1.0.0-rc-1",
    "gh-got": "^1.1.0",
    "istanbul": "^0.3.14",
    "markdownlint": "0.0.5",
    "npm-license": "^0.2.3",
    "pre-git": "^0.6.2",
    "prompt": "~0.2.11",
    "semver": "^4.3.6",
    "shelljs": "^0.3.0",
    "shelljs-nodecli": "^0.1.1",
    "vows": "~0.5.x"
  },
  "main": "./lib/spectcl",
  "scripts": {
    "test": "istanbul cover vows -- --spec && istanbul check-coverage --statement 81 --branch 78 --function 88 --line 81",
    "lint": "eslint lib/*.js test/*.js"
  },
  "pre-commit": [
    "npm run lint"
  ],
  "pre-push": [
    "npm test"
  ],
  "post-merge": [
    "npm install",
    "npm test"
  ],
  "engines": {
    "node": ">=0.8.0"
  },
  "license": "MIT"
}
